		"request": {
		  "method": "POST",
		  "header": [],
		  "body": {
			"mode": "raw",
			"raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john.doe@example.com\",\n  \"password\": \"123456\",\n  \"level\": 1\n}",
			"options": {
			  "raw": {
				"language": "json"
			  }
			}
		  },
		  "url": {
			"raw": "{{url}}/users",
			"host": [
			  "{{url}}"
			],
			"path": [
			  "users"
			]
		  }
		}
	  },
	  {
		"name": "Login User",
		"request": {
		  "method": "POST",
		  "header": [],
		  "body": {
			"mode": "raw",
			"raw": "{\n  \"email\": \"john.doe@example.com\",\n  \"password\": \"123456\"\n}",
			"options": {
			  "raw": {
				"language": "json"
			  }
			}
		  },
		  "url": {
			"raw": "{{url}}/login",
			"host": [
			  "{{url}}"
			],
			"path": [
			  "login"
			]
		  }
		}
	  },
	  {
		"name": "List All Users",
		"request": {
		  "method": "GET",
		  "header": [],
		  "url": {
			"raw": "{{url}}/users",
			"host": [
			  "{{url}}"
			],
			"path": [
			  "users"
			]
		  }
		}
	  },
	  {
		"name": "Get User By ID",
		"request": {
		  "method": "GET",
		  "header": [],
		  "url": {
			"raw": "{{url}}/users/{{userId}}",
			"host": [
			  "{{url}}"
			],
			"path": [
			  "users",
			  "{{userId}}"
			]
		  }
		}
	  },
	  {
		"name": "Update User By ID",
		"request": {
		  "method": "PUT",
		  "header": [],
		  "body": {
			"mode": "raw",
			"raw": "{\n  \"name\": \"Jane Doe\",\n  \"email\": \"jane.doe@example.com\",\n  \"level\": 2\n}",
			"options": {
			  "raw": {
				"language": "json"
			  }
			}
		  },
		  "url": {
			"raw": "{{url}}/users/{{userId}}",
			"host": [
			  "{{url}}"
			],
			"path": [
			  "users",
			  "{{userId}}"
			]
		  }
		}
	  },
	  {
		"name": "Delete (Deactivate) User By ID",
		"request": {
		  "method": "DELETE",
		  "header": [],
		  "url": {
			"raw": "{{url}}/users/{{userId}}",
			"host": [
			  "{{url}}"
			],
			"path": [
			  "users",
			  "{{userId}}"
			]
		  }
		}
	  },
	  {
		"name": "Generate User Report",
		"request": {
		  "method": "GET",
		  "header": [
			{
			  "key": "Authorization",
			  "value": "Bearer {{authToken}}",
			  "type": "text"
			}
		  ],
		  "url": {
			"raw": "{{url}}/users/report",
			"host": [
			  "{{url}}"
			],
			"path": [
			  "users",
			  "report"
			]
		  }
		}
	  }
	]
